          "_customHTML": "<section class=\"features5\" group=\"Features\" plugins=\"amp-main\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"4\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <header>Cards</header>\n        <input type=\"range\" inline title=\"Count\" name=\"cardsCount\" min=\"1\" max=\"3\" step=\"1\" value=\"3\" condition=\"showCardsTitle || showCardsText || showCardsButtons\" required>\n        <input type=\"checkbox\" title=\"Show Title\" name=\"showCardsTitle\" checked>\n        <input type=\"color\" title=\"Icon Color\" value=\"#05afec\" name=\"iconColor\">\n        <input type=\"color\" title=\"Icon Background\" value=\"#ffffff\" name=\"iconBg\">\n        <input type=\"image\" name=\"card1\" title=\"Card 1 BG Image\" value=\"@PROJECT_PATH@/assets/images/people10.jpg\" condition=\"cardsCount > 0\">\n        <input type=\"image\" name=\"card2\" title=\"Card 2 BG Image\" value=\"@PROJECT_PATH@/assets/images/people21.jpg\" condition=\"cardsCount > 1\">\n        <input type=\"image\" name=\"card3\" title=\"Card 3 BG Image\" value=\"@PROJECT_PATH@/assets/images/people32.jpg\" condition=\"cardsCount > 2\">\n        <input type=\"checkbox\" title=\"Transparent BG\" name=\"transparentBg\" condition=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons\">\n        <input type=\"range\" title=\"Opacity\" name=\"bgOpacity\" min=\"0\" max=\"1\" value=\"0.4\" step=\"0.1\" condition=\"!transparentBg\">\n        <input type=\"color\" title=\"Color\" name=\"cardBg\" value=\"#14212B\" condition=\"!transparentBg && (showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons)\">\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" required>\n            <input type=\"color\" title=\"Color\" value=\"#fef3f5\" selected>\n            <input type=\"image\" title=\"Image\" value=\"@PROJECT_PATH@/assets/images/background-1.jpg\">\n        </fieldset>\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" checked condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#efefef\" condition=\"overlay && bg.type == 'image'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type == 'image'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <div mbr-class=\"{'container':!fullWidth,\n                     'container-fluid':fullWidth}\">\n        <div class=\"mbr-row mbr-jc-c\">\n            <div class=\"card mbr-col-md-6 mbr-col-lg-4\">\n                <div class=\"card-wrapper card1\">\n                    <div class=\"card-img\">\n                        <a href=\"#\" target=\"_blank\" class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"amp-iconfont animation-normal mobi-mbri-photo mobi-mbri\"></span>\n                        </a>\n                    </div>\n                    <div class=\"card-box\">\n                        <div class=\"card-box-wrapper\">\n                            <h3 class=\"card-title mbr-regular mbr-white mbr-pb-2 mbr-fonts-style\" mbr-theme-style=\"display-5\" mbr-if=\"showCardsTitle\" data-app-selector=\".card-title, .card-btn, .card-img\">\n                                Business\n                            <div>Planner</div></h3>\n                        <div class=\"arrow-card\"></div>\n                        <p class=\"mbr-text mbr-regular  mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".link-align, .mbr-text\">\n                             Lorem ipsum dolor sit\n                            amet,\n                            consectetur adipiscing elit.\n                            Aliquam lobortis nisl ac augue tristique\n                        </p>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"card mbr-col-sm-12 mbr-col-md-6 mbr-col-lg-4\" mbr-if=\"cardsCount > 1\">\n                <div class=\"card-wrapper card2\">\n                    <div class=\"card-img\">\n                        <a href=\"#\" target=\"_blank\" class=\"iconfont-wrapper\">\n                            <span mbr-icon class=\"amp-iconfont animation-normal mobi-mbri-growing-chart mobi-mbri\"></span>\n                        </a>\n                    </div>\n                    <div class=\"card-box\">\n                        <div class=\"card-box-wrapper\">\n                            <h3 class=\"card-title mbr-regular mbr-pb-2 mbr-fonts-style mbr-white\" mbr-theme-style=\"display-5\" mbr-if=\"showCardsTitle\" data-app-selector=\".card-title, .card-btn, .card-img\">Awesome \n<div>Support</div></h3>\n                            <div class=\"arrow-card\"></div>\n                            <p class=\"mbr-text mbr-regular  mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".link-align, .mbr-text\">\n                                Lorem ipsum dolor sit\n                                amet,\n                                consectetur adipiscing elit.\n                                Aliquam lobortis nisl ac augue tristique\n                            </p>\n                        </div>\n                    </div>\n                </div>\n            </div>\n            <div class=\"card mbr-col-sm-12 mbr-col-md-6 mbr-col-lg-4\" mbr-if=\"cardsCount > 2\">\n                <div class=\"card-wrapper card3\">\n                    <div class=\"card-img\">\n                        <a href=\"#\" target=\"_blank\" class=\"iconfont-wrapper\">\n         